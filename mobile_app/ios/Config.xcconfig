// TeslaAI Genesis — Secure iOS Config (prod/qa/dev)
#include? "Secrets.xcconfig"

ENVIRONMENT = production
AI_MODE = secure-distributed

// Base App Info
PRODUCT_NAME = TeslaAI
BUNDLE_IDENTIFIER = ai.teslagenesis.app
DEVELOPMENT_TEAM = Z8Q1T1GEN1
PROVISIONING_PROFILE_SPECIFIER = TeslaAI_Profile_Production
CODE_SIGN_STYLE = Manual

// Build Numbers
MARKETING_VERSION = 2.1.0
CURRENT_PROJECT_VERSION = 210

// Feature Flags
ENABLE_AI_OVERLAY = YES
ENABLE_AI_GOVERNOR = YES
ENABLE_PRIVACY_SANDBOX = YES
ENABLE_ANALYTICS_FIREWALL = YES
ENABLE_TOR_PROXY = YES
ENABLE_ZERO_TRUST = YES

// Compiler Flags
OTHER_SWIFT_FLAGS = $(inherited) -DENABLE_AI_LOGGING -DUSE_SECURE_STORAGE

// Paths
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon
ASSETCATALOG_COMPILER_LAUNCHIMAGE_NAME = LaunchImage
SWIFT_OBJC_BRIDGING_HEADER = $(SRCROOT)/ios/Shared/Bridging-Header.h
INFOPLIST_FILE = $(SRCROOT)/ios/Info.plist

// Optimization
ENABLE_BITCODE = NO
SWIFT_OPTIMIZATION_LEVEL = -Owholemodule
SWIFT_COMPILATION_MODE = wholemodule

// Logging / Privacy
TESLAAI_PRIVACY_MODE = strict
TESLAAI_LOGGING_LEVEL = warning
TESLAAI_METRIC_EXPORT = secure-only

// Gateways
AI_INTENT_RESOLVER_URL = https://intent-gate.teslaai.net
AI_GOVERNOR_URL = https://governor.teslaai.net
PRIVACY_CORE_URL = https://privacy.teslaai.net

// Secrets: Зашифрованные ключи (из Secrets.xcconfig, не коммитятся)
