{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "iteration": 1757501000000,
  "links": [],
  "liveNow": false,
  "refresh": "15s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["k8s", "etcd", "observability", "latency", "rps", "storage"],
  "timezone": "",
  "title": "Kubernetes • etcd (lat/ops/db size)",
  "uid": "k8s-etcd-industrial-01",
  "version": 1,

  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "options": [],
        "hide": 0
      },
      {
        "type": "query",
        "name": "job",
        "label": "job",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(up{__name__=~\"etcd.*|grpc.*\"}, job)",
        "refresh": 2,
        "hide": 0,
        "current": { "selected": true, "text": "etcd", "value": "etcd" }
      },
      {
        "type": "query",
        "name": "namespace",
        "label": "namespace",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(up{job=~\"$job\"}, namespace)",
        "refresh": 2,
        "hide": 0,
        "current": { "selected": false, "text": ".*", "value": ".*" }
      },
      {
        "type": "query",
        "name": "instance",
        "label": "instance",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(up{job=~\"$job\", namespace=~\"$namespace\"}, instance)",
        "refresh": 2,
        "hide": 0,
        "current": { "selected": false, "text": ".*", "value": ".*" }
      },
      {
        "type": "custom",
        "name": "quantile",
        "label": "quantile",
        "query": "0.95,0.99",
        "options": [
          { "value": "0.95", "text": "0.95", "selected": true },
          { "value": "0.99", "text": "0.99" }
        ],
        "current": { "text": "0.95", "value": "0.95" }
      }
    ]
  },

  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","15s","30s","1m","2m","5m"] },

  "panels": [
    {
      "type": "stat",
      "title": "Cluster • Has Leader (fraction of nodes)",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Доля членов кластера, заявляющих наличие лидера.",
      "fieldConfig": { "defaults": { "unit": "percentunit", "decimals": 0, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [ { "color": "red" }, { "color": "orange", "value": 0.66 }, { "color": "green", "value": 0.9 } ] } }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "range": true,
          "expr": "avg(etcd_server_has_leader{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"})"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Leader Changes / min",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Скорость смен лидера (по всем узлам).",
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 2, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 0.1 }, { "color": "red", "value": 0.5 } ] } } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum(rate(etcd_server_leader_changes_seen_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[5m]))" }
      ]
    },
    {
      "type": "stat",
      "title": "Pending Proposals",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Количество неподтверждённых предложений (сумма).",
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 50 }, { "color": "red", "value": 200 } ] } } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum(etcd_server_proposals_pending{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"})" }
      ]
    },
    {
      "type": "stat",
      "title": "DB Size (sum)",
      "id": 4,
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Суммарный размер backend БД (байты).",
      "fieldConfig": { "defaults": { "unit": "decbytes", "decimals": 0 } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum(etcd_mvcc_db_total_size_in_bytes{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"})" }
      ]
    },

    {
      "type": "timeseries",
      "title": "Client RPS (get/put/txn/compaction)",
      "id": 10,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Запросы клиентов к etcd по типам.",
      "fieldConfig": { "defaults": { "unit": "ops" } },
      "options": { "legend": { "showLegend": true, "placement": "right", "displayMode": "table" } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum by (instance) (rate(etcd_request_duration_seconds_count{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\",type=\"range\"}[$__rate_interval]))", "legendFormat": "get ({{instance}})" },
        { "refId": "B", "range": true, "expr": "sum by (instance) (rate(etcd_request_duration_seconds_count{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\",type=\"put\"}[$__rate_interval]))", "legendFormat": "put ({{instance}})" },
        { "refId": "C", "range": true, "expr": "sum by (instance) (rate(etcd_request_duration_seconds_count{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\",type=\"txn\"}[$__rate_interval]))", "legendFormat": "txn ({{instance}})" },
        { "refId": "D", "range": true, "expr": "sum by (instance) (rate(etcd_debugging_mvcc_db_compaction_keys_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "compaction keys ({{instance}})" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Client Errors (% of total)",
      "id": 11,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Процент ошибок по клиентским запросам (etcd_request_errors_total/total).",
      "fieldConfig": { "defaults": { "unit": "percentunit", "decimals": 2 } },
      "options": { "legend": { "showLegend": true, "placement": "right", "displayMode": "table" } },
      "targets": [
        {
          "refId": "A",
          "range": true,
          "expr": "100 * (sum by (instance) (rate(etcd_request_errors_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval])) / sum by (instance) (rate(etcd_request_duration_seconds_count{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval])))",
          "legendFormat": "error % ({{instance}})"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "Latency p${quantile} • Client requests (histogram)",
      "id": 20,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Квантиль латентности клиентских запросов (histogram_quantile по etcd_request_duration_seconds_bucket).",
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 } },
      "options": { "legend": { "showLegend": true, "placement": "right", "displayMode": "table" } },
      "targets": [
        {
          "refId": "A",
          "range": true,
          "expr": "histogram_quantile(${quantile}, sum by (le, instance) (rate(etcd_request_duration_seconds_bucket{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval])))",
          "legendFormat": "p${quantile} ({{instance}})"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p${quantile} • WAL fsync",
      "id": 21,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Латентность fsync WAL (histogram_quantile по etcd_disk_wal_fsync_duration_seconds_bucket).",
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 } },
      "targets": [
        { "refId": "A", "range": true, "expr": "histogram_quantile(${quantile}, sum by (le, instance) (rate(etcd_disk_wal_fsync_duration_seconds_bucket{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval])))", "legendFormat": "fsync p${quantile} ({{instance}})" }
      ]
    },

    {
      "type": "timeseries",
      "title": "Latency p${quantile} • Backend commit",
      "id": 22,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Латентность коммитов backend (histogram_quantile по etcd_disk_backend_commit_duration_seconds_bucket).",
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 } },
      "targets": [
        { "refId": "A", "range": true, "expr": "histogram_quantile(${quantile}, sum by (le, instance) (rate(etcd_disk_backend_commit_duration_seconds_bucket{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval])))", "legendFormat": "commit p${quantile} ({{instance}})" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Proposals: committed/applied RPS",
      "id": 23,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Скорость коммитов и применений предложений.",
      "fieldConfig": { "defaults": { "unit": "ops" } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum by (instance) (rate(etcd_server_proposals_committed_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "committed ({{instance}})" },
        { "refId": "B", "range": true, "expr": "sum by (instance) (rate(etcd_server_proposals_applied_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "applied ({{instance}})" }
      ]
    },

    {
      "type": "timeseries",
      "title": "Peer traffic (recv/sent bytes)",
      "id": 30,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Сетевой трафик peer-to-peer между членами кластера.",
      "fieldConfig": { "defaults": { "unit": "Bps" } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum by (instance) (rate(etcd_network_peer_received_bytes_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "peer recv ({{instance}})" },
        { "refId": "B", "range": true, "expr": "sum by (instance) (rate(etcd_network_peer_sent_bytes_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "peer sent ({{instance}})" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Client traffic (recv/sent bytes)",
      "id": 31,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Клиентский сетевой трафик (gRPC/HTTP) к etcd.",
      "fieldConfig": { "defaults": { "unit": "Bps" } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum by (instance) (rate(etcd_network_client_grpc_received_bytes_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "client recv ({{instance}})" },
        { "refId": "B", "range": true, "expr": "sum by (instance) (rate(etcd_network_client_grpc_sent_bytes_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "client sent ({{instance}})" }
      ]
    },

    {
      "type": "timeseries",
      "title": "gRPC server connections (open)",
      "id": 40,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Число открытых gRPC-соединений.",
      "fieldConfig": { "defaults": { "unit": "short" } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum by (instance) (grpc_server_connections{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"})", "legendFormat": "connections ({{instance}})" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Heartbeat failures & fsync errors",
      "id": 41,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Неудачные heartbeat и ошибки fsync (rate).",
      "fieldConfig": { "defaults": { "unit": "ops" } },
      "targets": [
        { "refId": "A", "range": true, "expr": "sum by (instance) (rate(etcd_server_heartbeat_send_failures_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "heartbeat fail ({{instance}})" },
        { "refId": "B", "range": true, "expr": "sum by (instance) (rate(etcd_debugging_disk_wal_fsync_errors_total{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}[$__rate_interval]))", "legendFormat": "fsync errors ({{instance}})" }
      ]
    },

    {
      "type": "stat",
      "title": "DB Size • per instance",
      "id": 50,
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 44 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Размер backend БД по инстансам.",
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 0 } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal", "colorMode": "value", "graphMode": "none", "justifyMode": "auto" },
      "targets": [
        { "refId": "A", "range": true, "expr": "etcd_mvcc_db_total_size_in_bytes{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}", "legendFormat": "{{instance}}" }
      ]
    },
    {
      "type": "stat",
      "title": "Backend quota (bytes) • per instance",
      "id": 51,
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 44 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Квота backend по инстансам.",
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 0 } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal" },
      "targets": [
        { "refId": "A", "range": true, "expr": "etcd_server_quota_backend_bytes{job=~\"$job\",namespace=~\"$namespace\",instance=~\"$instance\"}", "legendFormat": "{{instance}}" }
      ]
    }
  ],

  "__inputs": [
    { "name": "DS_PROMETHEUS", "label": "Prometheus", "description": "", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.4.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.9.0" }
  ]
}
