{
  "uid": "sample-test",
  "title": "Sample Test — Grafana JSON Lint & Smoke",
  "tags": ["test", "lint", "smoke", "aethernova"],
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "refresh": "30s",
  "timezone": "",
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": { "refresh_intervals": ["10s", "30s", "1m", "5m", "10m"] },
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "Smoke",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Targets UP",
      "description": "Количество целей в состоянии UP по выбранным фильтрам.",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        {
          "refId": "A",
          "instant": true,
          "expr": "sum(up{job=~\"$job\", instance=~\"$instance\"}) or on() vector(0)"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "noValue": "0"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "red" }, { "color": "green", "value": 1 }]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Targets DOWN",
      "description": "Количество целей в состоянии DOWN по выбранным фильтрам.",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        {
          "refId": "A",
          "instant": true,
          "expr": "sum((up{job=~\"$job\", instance=~\"$instance\"} == 0)) or on() vector(0)"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "noValue": "0"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green" }, { "color": "red", "value": 1 }]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Datasource OK",
      "description": "Быстрая проверка доступности Prometheus (есть ли хоть одна серия up).",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        { "refId": "A", "instant": true, "expr": "count(up) > 0" }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "noValue": "false"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bool",
          "mappings": [
            { "type": "value", "options": { "0": { "text": "false", "color": "red" }, "1": { "text": "true", "color": "green" } } }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Series count (filtered)",
      "description": "Число серий up после фильтров шаблонов.",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        { "refId": "A", "instant": true, "expr": "count(up{job=~\"$job\", instance=~\"$instance\"}) or on() vector(0)" }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "noValue": "0"
      },
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
    },
    {
      "type": "row",
      "title": "Series & Latency",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "UP by instance",
      "description": "Значение метрики up по инстансам.",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=~\"$job\", instance=~\"$instance\"}",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none", "decimals": 0 },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "timeseries",
      "title": "Scrape duration (p50/p95/p99)",
      "description": "Квантили длительности скрапа (если метрика доступна).",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        {
          "refId": "P50",
          "expr": "histogram_quantile(0.50, sum by (le) (rate(scrape_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[$__rate_interval])))",
          "legendFormat": "p50"
        },
        {
          "refId": "P95",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(scrape_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[$__rate_interval])))",
          "legendFormat": "p95"
        },
        {
          "refId": "P99",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(scrape_duration_seconds_bucket{job=~\"$job\", instance=~\"$instance\"}[$__rate_interval])))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } }
    },
    {
      "type": "row",
      "title": "Table view",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "table",
      "title": "Instances — last sample",
      "description": "Сводка up и времени последнего образца по instance.",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
      "targets": [
        { "refId": "A", "expr": "up{job=~\"$job\", instance=~\"$instance\"}", "instant": true, "legendFormat": "{{instance}}" },
        { "refId": "B", "expr": "timestamp(up{job=~\"$job\", instance=~\"$instance\"})", "instant": true }
      ],
      "transformations": [
        { "id": "seriesToColumns", "options": { "byField": "instance" } },
        { "id": "organize", "options": {
          "renameByName": { "Value #A": "up", "Value #B": "last_sample" }
        } }
      ],
      "options": { "showHeader": true },
      "fieldConfig": {
        "defaults": { "custom": { "align": "center" } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "up" }, "properties": [
            { "id": "mappings", "value": [
              { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
            ] }
          ] },
          { "matcher": { "id": "byName", "options": "last_sample" }, "properties": [
            { "id": "unit", "value": "dateTimeAsIso" }
          ] }
        ]
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS_UID",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": true },
        "label": "Datasource",
        "options": []
      },
      {
        "name": "job",
        "type": "query",
        "label": "job",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "query": "label_values(up, job)",
        "current": { "selected": true, "value": [".*"], "text": "All" }
      },
      {
        "name": "instance",
        "type": "query",
        "label": "instance",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS_UID}" },
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "current": { "selected": true, "value": [".*"], "text": "All" }
      }
    ]
  },
  "annotations": {
    "list": [
      { "type": "dashboard", "name": "Annotations & Alerts", "hide": true, "builtIn": 1, "iconColor": "rgba(255, 255, 255, 0.6)" }
    ]
  }
}
