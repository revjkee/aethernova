{
  "id": null,
  "uid": "consensus-peers",
  "title": "Consensus: Peers",
  "timezone": "",
  "version": 1,
  "schemaVersion": 39,
  "editable": true,
  "style": "dark",
  "refresh": "30s",
  "tags": ["consensus", "peers", "validators"],
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": { "refresh_intervals": ["10s", "30s", "1m", "5m"], "hidden": false },
  "fiscalYearStartMonth": 0,
  "weekStart": "",
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Active peers (current)",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum($peer_count_metric{job=~\"$job\",instance=~\"$instance\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [ { "color": "red", "value": null }, { "color": "green", "value": 1 } ] }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }, "orientation": "auto", "text": {}, "graphMode": "none", "justifyMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Handshake success rate (ops/s)",
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(rate($handshake_success_total_metric{job=~\"$job\",instance=~\"$instance\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2 }, "overrides": [] },
      "options": { "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }, "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Handshake failure rate (ops/s)",
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(rate($handshake_failure_total_metric{job=~\"$job\",instance=~\"$instance\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2 }, "overrides": [] },
      "options": { "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }, "colorMode": "value" }
    },
    {
      "type": "timeseries",
      "title": "Handshakes: success vs failure (rate)",
      "id": 4,
      "transparent": true,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum by (instance) (rate($handshake_success_total_metric{job=~\"$job\",instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "{{instance}} success"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "sum by (instance) (rate($handshake_failure_total_metric{job=~\"$job\",instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "{{instance}} failure"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2, "fillOpacity": 8, "showPoints": "never", "spanNulls": true }
        },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull","max","min","mean"] }, "tooltip": { "mode": "multi", "sort": "desc" } }
    },
    {
      "type": "bar-gauge",
      "title": "Disconnects by peer (rate)",
      "id": 5,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 13 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "topk(10, sum by (peer) (rate($disconnect_total_metric{job=~\"$job\",instance=~\"$instance\",peer=~\"$peer\"}[$__rate_interval])))",
          "legendFormat": "{{peer}}"
        }
      ],
      "options": { "displayMode": "gradient", "minVizWidth": 0, "minVizHeight": 10, "reduceOptions": { "calcs": ["lastNotNull"], "values": false } },
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2 }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Peer latency (avg)",
      "id": 6,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 13 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "avg by (peer) ($peer_latency_metric{job=~\"$job\",instance=~\"$instance\",peer=~\"$peer\"})",
          "legendFormat": "{{peer}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2, "fillOpacity": 6, "showPoints": "never", "spanNulls": true }
        },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "desc" } }
    },
    {
      "type": "timeseries",
      "title": "Dropped messages (rate)",
      "id": 7,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum by (peer) (rate($drops_total_metric{job=~\"$job\",instance=~\"$instance\",peer=~\"$peer\"}[$__rate_interval]))",
          "legendFormat": "{{peer}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10, "showPoints": "never", "spanNulls": true }
        },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull","max","min","mean"] }, "tooltip": { "mode": "multi", "sort": "desc" } }
    },
    {
      "type": "table",
      "title": "Handshake errors (last 5m, top 10)",
      "id": 8,
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "topk(10, sum by (peer,error) (rate($handshake_failure_total_metric{job=~\"$job\",instance=~\"$instance\",peer=~\"$peer\"}[5m])))"
        }
      ],
      "transformations": [
        { "id": "labelsToFields", "options": { "mode": "columns" } },
        { "id": "organize", "options": { "excludeByName": {}, "indexByName": {}, "renameByName": { "Value": "rate" } } }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2 }, "overrides": [] },
      "options": { "showHeader": true }
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROM",
        "label": "Prometheus",
        "query": "prometheus",
        "hide": 0
      },
      {
        "type": "query",
        "name": "job",
        "label": "job",
        "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
        "query": "label_values(up, job)",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "type": "query",
        "name": "instance",
        "label": "instance",
        "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "type": "textbox",
        "name": "peer",
        "label": "peer regex",
        "value": ".*",
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "peer_count_metric",
        "label": "metric: peer count (gauge)",
        "value": "PLACEHOLDER_peer_count",
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "handshake_success_total_metric",
        "label": "metric: handshake success _total (counter)",
        "value": "PLACEHOLDER_handshake_success_total",
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "handshake_failure_total_metric",
        "label": "metric: handshake failure _total (counter)",
        "value": "PLACEHOLDER_handshake_failure_total",
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "disconnect_total_metric",
        "label": "metric: disconnect _total (counter)",
        "value": "PLACEHOLDER_disconnect_total",
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "peer_latency_metric",
        "label": "metric: peer latency (gauge or summary)",
        "value": "PLACEHOLDER_peer_latency_seconds",
        "hide": 0
      },
      {
        "type": "textbox",
        "name": "drops_total_metric",
        "label": "metric: dropped messages _total (counter)",
        "value": "PLACEHOLDER_dropped_messages_total",
        "hide": 0
      }
    ]
  },
  "annotations": { "list": [] }
}
