  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "label": "Prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus" }
      },
      {
        "name": "job",
        "label": "job",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
        "query": "label_values(up, job)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "selected": false, "text": "All", "value": "$__all" }
      },
      {
        "name": "instance",
        "label": "instance",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "selected": false, "text": "All", "value": "$__all" }
      },
      {
        "name": "job_label",
        "type": "constant",
        "label": "job label key",
        "query": "job",
        "current": { "text": "job", "value": "job" }
      },
      {
        "name": "instance_label",
        "type": "constant",
        "label": "instance label key",
        "query": "instance",
        "current": { "text": "instance", "value": "instance" }
      },
      {
        "name": "window",
        "type": "custom",
        "label": "Окно расчета",
        "query": "1m,5m,10m,30m,1h",
        "current": { "text": "5m", "value": "5m" }
      },
      {
        "name": "gwei_div",
        "type": "constant",
        "label": "Делитель для gwei (wei→gwei = 1e9)",
        "query": "1e9",
        "current": { "text": "1e9", "value": "1e9" }
      },
      {
        "name": "metric_txpool_pending",
        "type": "constant",
        "label": "Metric: pending gauge",
