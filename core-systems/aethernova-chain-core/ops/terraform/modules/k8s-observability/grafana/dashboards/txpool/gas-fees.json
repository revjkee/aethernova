        "current": { "text": "txpool_pending_transactions", "value": "txpool_pending_transactions" },
        "description": "Имя метрики количества pending транзакций (gauge)."
      },
      {
        "name": "metric_txpool_queued",
        "type": "constant",
        "label": "Metric: queued gauge",
