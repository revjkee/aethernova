{
  "id": null,
  "uid": "rpc-latency-by-method",
  "title": "RPC â€” Latency by Method (p50/p90/p99, Heatmap, RPS, Errors)",
  "tags": ["observability", "rpc", "latency", "aethernova", "production"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "graphTooltip": 1,
  "style": "dark",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s","10s","15s","30s","1m","5m","15m"]
  },
  "annotations": {
    "list": []
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "label": "Prometheus",
        "refresh": 1
      },
      {
        "name": "job",
        "type": "query",
        "datasource": "${datasource}",
        "label": "job",
        "query": "label_values(rpc_request_duration_seconds_bucket, job)",
        "refresh": 2,
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false
      },
      {
        "name": "service",
        "type": "query",
        "datasource": "${datasource}",
        "label": "service",
        "query": "label_values(rpc_request_duration_seconds_bucket{job=\"$job\"}, service)",
        "refresh": 2,
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "method",
        "type": "query",
        "datasource": "${datasource}",
        "label": "method",
        "query": "label_values(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\"}, method)",
        "refresh": 2,
        "current": {},
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": "${datasource}",
        "label": "instance",
        "query": "label_values(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\"}, instance)",
        "refresh": 2,
        "current": {},
        "hide": 2,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "quantiles",
        "type": "custom",
        "label": "quantiles",
        "query": "0.5,0.9,0.99",
        "current": { "text": "0.5,0.9,0.99", "value": "0.5,0.9,0.99" },
        "hide": 2,
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Latency p50/p90/p99 by method",
      "datasource": "${datasource}",
      "interval": "$__interval",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum by (le, method) (rate(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])))",
          "legendFormat": "p50 {{method}}"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.9, sum by (le, method) (rate(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])))",
          "legendFormat": "p90 {{method}}"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum by (le, method) (rate(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])))",
          "legendFormat": "p99 {{method}}"
        }
      ],
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "heatmap",
      "title": "Latency distribution heatmap (by method)",
      "datasource": "${datasource}",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true }, "tooltip": { "show": true } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (le) (rate(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval]))",
          "format": "heatmap"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 9 }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Requests per second (RPS) by method",
      "datasource": "${datasource}",
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (method) (rate(rpc_requests_total{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval]))",
          "legendFormat": "{{method}}"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 9 }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Error rate by method (5xx / non-OK)",
      "datasource": "${datasource}",
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (method) (rate(rpc_requests_errors_total{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval]))",
          "legendFormat": "{{method}} errors"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 }
    },
    {
      "id": 5,
      "type": "bargauge",
      "title": "TOP methods by avg latency (rolling)",
      "datasource": "${datasource}",
      "options": { "displayMode": "gradient", "orientation": "horizontal" },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (method) (rate(rpc_request_duration_seconds_sum{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])) / sum by (method) (rate(rpc_request_duration_seconds_count{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])))",
          "legendFormat": "{{method}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 }
    },
    {
      "id": 6,
      "type": "table",
      "title": "Slow methods table (avg, p99, RPS, error%)",
      "datasource": "${datasource}",
      "options": { "showHeader": true },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (method) (rate(rpc_request_duration_seconds_sum{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])) / sum by (method) (rate(rpc_request_duration_seconds_count{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval]))",
          "legendFormat": "avg_latency_s"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.99, sum by (le, method) (rate(rpc_request_duration_seconds_bucket{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])))",
          "legendFormat": "p99_latency_s"
        },
        {
          "refId": "C",
          "expr": "sum by (method) (rate(rpc_requests_total{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval]))",
          "legendFormat": "rps"
        },
        {
          "refId": "D",
          "expr": "100 * (sum by (method) (rate(rpc_requests_errors_total{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])) / sum by (method) (rate(rpc_requests_total{job=\"$job\",service=~\"$service\",method=~\"$method\"}[$__rate_interval])))",
          "legendFormat": "error_pct"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "organize", "options": { "excludeByName": {}, "indexByName": {}, "renameByName": {} } }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "error_pct" }, "properties": [ { "id": "unit", "value": "percent" } ] },
          { "matcher": { "id": "byName", "options": "avg_latency_s" }, "properties": [ { "id": "unit", "value": "s" } ] },
          { "matcher": { "id": "byName", "options": "p99_latency_s" }, "properties": [ { "id": "unit", "value": "s" } ] }
        ]
      },
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 26 }
    }
  ]
}
