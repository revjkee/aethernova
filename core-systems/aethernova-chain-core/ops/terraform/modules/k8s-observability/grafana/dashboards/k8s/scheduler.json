{
  "uid": "k8s-scheduler-overview",
  "title": "Kubernetes / Scheduler / Latency & Queues",
  "tags": ["kubernetes", "scheduler", "latency", "control-plane"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-6h", "to": "now" },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "style": "dark",
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "label": "Prometheus",
        "current": { "text": "", "value": "" },
        "refresh": 1
      },
      {
        "type": "query",
        "name": "job",
        "label": "job",
        "includeAll": true,
        "multi": true,
        "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
        "query": "label_values(scheduler_scheduling_attempt_duration_seconds_count, job)",
        "current": { "text": "kube-scheduler", "value": "kube-scheduler" }
      },
      {
        "type": "query",
        "name": "instance",
        "label": "instance",
        "includeAll": true,
        "multi": true,
        "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
        "query": "label_values(scheduler_scheduling_attempt_duration_seconds_count{job=~\"$job\"}, instance)",
        "current": { "text": "All", "value": ".*" }
      },
      {
        "type": "constant",
        "name": "rate_interval",
        "label": "rate() window",
        "query": "5m",
        "current": { "text": "5m", "value": "5m" }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard",
        "datasource": { "type": "grafana", "uid": "-- Grafana --" }
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "E2E scheduling latency",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 1,
      "type": "stat",
      "title": "E2E p99 (s)",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "instant": true,
          "expr": "histogram_quantile(0.99, sum by (le) (rate(scheduler_e2e_scheduling_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value", "textMode": "value" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 0.25 }, { "color": "red", "value": 1 } ] }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "E2E latency p50 / p90 / p99",
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "P50", "expr": "histogram_quantile(0.5,  sum by (le) (rate(scheduler_e2e_scheduling_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" },
        { "refId": "P90", "expr": "histogram_quantile(0.9,  sum by (le) (rate(scheduler_e2e_scheduling_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" },
        { "refId": "P99", "expr": "histogram_quantile(0.99, sum by (le) (rate(scheduler_e2e_scheduling_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Algorithm vs Binding latency (p99)",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "ALG", "expr": "histogram_quantile(0.99, sum by (le) (rate(scheduler_scheduling_algorithm_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))", "legendFormat": "algorithm p99" },
        { "refId": "BIND", "expr": "histogram_quantile(0.99, sum by (le) (rate(scheduler_binding_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))", "legendFormat": "binding p99" }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] }
    },
    {
      "type": "row",
      "title": "Queues & Attempts",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Pending pods (unschedulable)",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "instant": true, "expr": "max(scheduler_pending_pods{queue=\"unschedulable\",job=~\"$job\",instance=~\"$instance\"})" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 10 }, { "color": "red", "value": 50 } ] }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Pending pods (backoff / active / gated)",
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "B", "instant": true, "expr": "max(scheduler_pending_pods{queue=\"backoff\",job=~\"$job\",instance=~\"$instance\"})", "legendFormat": "backoff" },
        { "refId": "C", "instant": true, "expr": "max(scheduler_pending_pods{queue=\"active\",job=~\"$job\",instance=~\"$instance\"})", "legendFormat": "active" },
        { "refId": "D", "instant": true, "expr": "max(scheduler_pending_pods{queue=\"gated\",job=~\"$job\",instance=~\"$instance\"})", "legendFormat": "gated" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "value" },
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Incoming pods to queues (rate)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "expr": "sum by (queue, event) (rate(scheduler_queue_incoming_pods_total{job=~\"$job\",instance=~\"$instance\"}[$rate_interval]))", "legendFormat": "{{queue}} {{event}}" }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Schedule attempts by result (rate)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "expr": "sum by (result) (rate(scheduler_schedule_attempts_total{job=~\"$job\",instance=~\"$instance\"}[$rate_interval]))", "legendFormat": "{{result}}" }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "type": "row",
      "title": "Attempt duration (stable) & SLI (beta)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Scheduling attempt duration (p50/p90/p99)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "P50", "expr": "histogram_quantile(0.5,  sum by (le) (rate(scheduler_scheduling_attempt_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" },
        { "refId": "P90", "expr": "histogram_quantile(0.9,  sum by (le) (rate(scheduler_scheduling_attempt_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" },
        { "refId": "P99", "expr": "histogram_quantile(0.99, sum by (le) (rate(scheduler_scheduling_attempt_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Pod scheduling SLI (beta, p50/p90/p99)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "S50", "expr": "histogram_quantile(0.5,  sum by (le) (rate(scheduler_pod_scheduling_sli_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" },
        { "refId": "S90", "expr": "histogram_quantile(0.9,  sum by (le) (rate(scheduler_pod_scheduling_sli_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" },
        { "refId": "S99", "expr": "histogram_quantile(0.99, sum by (le) (rate(scheduler_pod_scheduling_sli_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\"}[$rate_interval])))" }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] }
    },
    {
      "type": "row",
      "title": "Framework extension points",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Extension point duration p99 by stage",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 37 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le, extension_point) (rate(scheduler_framework_extension_point_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\",status=\"success\"}[$rate_interval])))",
          "legendFormat": "{{extension_point}} p99"
        }
      ],
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] }
    }
  ],
  "plugins": [],
  "links": [],
  "panelsState": {},
  "timepicker": { "hidden": false }
}
