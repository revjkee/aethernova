{
  "id": null,
  "uid": "zk-prover-perf",
  "title": "ZK Prover Performance",
  "tags": ["aethernova", "zk", "prover", "observability", "gpu"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","30s","1m","5m"] },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "ds_prom",
        "label": "Prometheus",
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus" }
      },
      {
        "type": "datasource",
        "name": "ds_loki",
        "label": "Loki",
        "query": "loki",
        "current": { "text": "Loki", "value": "Loki" }
      },
      {
        "type": "query",
        "name": "job",
        "label": "job",
        "datasource": { "type": "prometheus", "uid": "$ds_prom" },
        "query": "label_values(up, job)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "type": "query",
        "name": "namespace",
        "label": "namespace",
        "datasource": { "type": "prometheus", "uid": "$ds_prom" },
        "query": "label_values(kube_pod_info, namespace)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "type": "query",
        "name": "pod",
        "label": "pod",
        "datasource": { "type": "prometheus", "uid": "$ds_prom" },
        "query": "label_values(kube_pod_info{namespace=~\"$namespace\"}, pod)",
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "type": "constant",
        "name": "zk_latency_histogram_bucket",
        "label": "latency_bucket",
        "query": "zk_prover_proof_seconds_bucket"
      },
      {
        "type": "constant",
        "name": "zk_throughput_counter",
        "label": "proofs_total",
        "query": "zk_prover_proofs_total"
      },
      {
        "type": "constant",
        "name": "zk_errors_counter",
        "label": "errors_total",
        "query": "zk_prover_errors_total"
      },
      {
        "type": "constant",
        "name": "queue_depth_gauge",
        "label": "queue_depth",
        "query": "zk_prover_queue_depth"
      },
      {
        "type": "constant",
        "name": "gpu_util_metric",
        "label": "gpu_util",
        "query": "DCGM_FI_DEV_GPU_UTIL"
      },
      {
        "type": "constant",
        "name": "gpu_mem_used_metric",
        "label": "gpu_mem_used",
        "query": "DCGM_FI_DEV_FB_USED"
      },
      {
        "type": "constant",
        "name": "gpu_temp_metric",
        "label": "gpu_temp",
        "query": "DCGM_FI_DEV_GPU_TEMP"
      }
    ]
  },
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Throughput proofs/s",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "sum(rate(${zk_throughput_counter}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval]))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal" }
    },
    {
      "type": "stat",
      "title": "Errors/s",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "sum(rate(${zk_errors_counter}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval]))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal" }
    },
    {
      "type": "timeseries",
      "title": "Latency percentiles p50 p90 p99",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "P50",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "histogram_quantile(0.5, sum by (le) (rate(${zk_latency_histogram_bucket}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval])))"
        },
        {
          "refId": "P90",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "histogram_quantile(0.9, sum by (le) (rate(${zk_latency_histogram_bucket}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval])))"
        },
        {
          "refId": "P99",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "histogram_quantile(0.99, sum by (le) (rate(${zk_latency_histogram_bucket}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval])))"
        }
      ]
    },
    {
      "type": "heatmap",
      "title": "Latency distribution heatmap",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "dataFormat": "tsbuckets",
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "sum by (le) (rate(${zk_latency_histogram_bucket}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Queue depth",
      "gridPos": { "x": 0, "y": 20, "w": 6, "h": 6 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "sum(${queue_depth_gauge}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"})"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "GPU utilization percent",
      "gridPos": { "x": 6, "y": 20, "w": 6, "h": 6 },
      "fieldConfig": { "defaults": { "unit": "percent" } },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "avg(${gpu_util_metric}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"})"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "GPU memory used MiB",
      "gridPos": { "x": 0, "y": 26, "w": 6, "h": 6 },
      "fieldConfig": { "defaults": { "unit": "mbytes" } },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "avg(${gpu_mem_used_metric}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"})"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "GPU temperature C",
      "gridPos": { "x": 6, "y": 26, "w": 6, "h": 6 },
      "fieldConfig": { "defaults": { "unit": "celsius" } },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "avg(${gpu_temp_metric}{job=~\"$job\",namespace=~\"$namespace\",pod=~\"$pod\"})"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top workers by latency p99",
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 7 },
      "transformations": [],
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "$ds_prom" },
          "expr": "topk(10, histogram_quantile(0.99, sum by (pod, le) (rate(${zk_latency_histogram_bucket}{namespace=~\"$namespace\",pod=~\"$pod\"}[$__rate_interval]))))"
        }
      ]
    },
    {
      "type": "logs",
      "title": "Errors from prover pods",
      "gridPos": { "x": 0, "y": 39, "w": 12, "h": 9 },
      "options": { "showTime": true },
      "datasource": { "type": "loki", "uid": "$ds_loki" },
      "targets": [
        {
          "refId": "A",
          "expr": "{namespace=~\"$namespace\", pod=~\"$pod\"} |= \"error\"",
          "queryType": "range"
        }
      ]
    }
  ]
}
