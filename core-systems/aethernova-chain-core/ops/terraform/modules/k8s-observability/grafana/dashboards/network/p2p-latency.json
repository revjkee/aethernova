          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "RTT per target (ms)",
      "id": 4,
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "description": "Значения RTT по целям (instance) во времени.",
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max", "mean"] },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "probe_icmp_duration_seconds{job=~\"$job\", module=~\"$module\", instance=~\"$instance\", phase=\"rtt\"} * 1000",
          "legendFormat": "{{instance}}",
          "exemplar": true
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Probe duration total (s)",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "description": "Общее время пробы (blackbox) — полезно для калибровки интервалов.",
      "fieldConfig": {
        "defaults": { "unit": "s", "decimals": 3 },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max", "mean"] },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "probe_duration_seconds{job=~\"$job\", module=~\"$module\", instance=~\"$instance\"}",
          "legendFormat": "{{instance}}",
          "exemplar": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "Success ratio (%)",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "description": "Доля успешных проб за окно $window (0–100%).",
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(probe_success{job=~\"$job\", module=~\"$module\", instance=~\"$instance\"}[$window]) * 100",
          "legendFormat": "success%",
          "exemplar": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 98 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "table",
      "title": "Top N worst RTT (max over $window, ms)",
      "id": 7,
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": { "Value": "max_rtt_ms" }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "max_rtt_ms", "desc": true }]
      },
      "fieldConfig": {
        "defaults": { "custom": { "align": "auto" }, "decimals": 2, "unit": "ms" },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk($topn, max_over_time(probe_icmp_duration_seconds{job=~\"$job\", module=~\"$module\", instance=~\"$instance\", phase=\"rtt\"}[$window]) * 1000)",
          "legendFormat": "{{instance}}",
          "exemplar": true
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["observability", "network", "p2p", "latency", "blackbox"],
  "templating": {
    "list": [
      {
        "name": "DS_PROM",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "hide": 0
      },
      {
        "name": "job",
        "type": "query",
        "label": "job",
        "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
        "refresh": 2,
        "query": "label_values(probe_success, job)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "sort": 1
      },
      {
        "name": "module",
        "type": "query",
        "label": "module",
        "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
        "refresh": 2,
        "query": "label_values(probe_success{job=~\"$job\"}, module)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "sort": 1
      },
      {
        "name": "instance",
        "type": "query",
        "label": "instance",
        "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
        "refresh": 2,
        "query": "label_values(probe_success{job=~\"$job\", module=~\"$module\"}, instance)",
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "sort": 1
      },
      {
        "name": "window",
        "type": "custom",
        "label": "window",
        "query": "5m,15m,30m,1h,2h,6h,12h,24h",
        "current": { "selected": true, "text": "15m", "value": "15m" }
      },
      {
        "name": "topn",
        "type": "custom",
        "label": "Top N",
        "query": "5,10,15,20",
        "current": { "selected": true, "text": "10", "value": "10" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s","10s","30s","1m","5m","15m","30m","1h","2h","1d"],
    "time_options": ["5m","15m","1h","6h","12h","24h","2d","7d","30d"]
  },
  "timezone": "",
  "title": "Network / P2P Latency (Blackbox)",
  "uid": "network-p2p-latency",
  "version": 1,
  "weekStart": ""
}
