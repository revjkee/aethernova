  "panels": [
    {
      "type": "stat",
      "title": "RTT p50 (ms)",
      "id": 1,
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "description": "Медианный RTT за выбранное окно $window для фильтра (job/module/instance).",
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.5, probe_icmp_duration_seconds{job=~\"$job\", module=~\"$module\", instance=~\"$instance\", phase=\"rtt\"}[$window]) * 1000",
          "legendFormat": "p50",
          "exemplar": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 300 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "RTT p90 (ms)",
      "id": 2,
