    {
      "type": "stat",
      "title": "RTT p99 (ms)",
      "id": 3,
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROM" },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.99, probe_icmp_duration_seconds{job=~\"$job\", module=~\"$module\", instance=~\"$instance\", phase=\"rtt\"}[$window]) * 1000",
          "legendFormat": "p99",
          "exemplar": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
