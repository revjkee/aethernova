      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max without (instance) (beacon_finalized_epoch{job=~\"$job\",instance=~\"$instance\"})",
          "legendFormat": "finalized_epoch",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] }
        },
        "overrides": []
      }
    },
    {
