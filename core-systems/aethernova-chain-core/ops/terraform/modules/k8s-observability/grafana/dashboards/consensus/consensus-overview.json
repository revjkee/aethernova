        {
          "refId": "A",
          "expr": "max without (instance) (floor(((slotclock_present_slot{job=~\"$job\",instance=~\"$instance\"} or beacon_clock_time_slot{job=~\"$job\",instance=~\"$instance\"}) / 32)))",
