{
  "uid": "tsdb-utilization",
  "title": "Prometheus TSDB Utilization",
  "description": "Промышленная панель для мониторинга использования TSDB у прометеев: объем блоков на диске, активные серии и чанки в head, скорость инжеста, загруженные блоки, GC head. Универсальна для Kubernetes/VM, поддерживает фильтры по job/namespace/pod/instance.",
  "tags": ["prometheus", "tsdb", "storage", "observability"],
  "timezone": "",
  "schemaVersion": 38,
  "version": 1,
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s","10s","30s","1m","5m","15m","30m","1h"],
    "hidden": false
  },
  "refresh": "30s",
  "fiscalYearStartMonth": 0,
  "editable": true,
  "graphTooltip": 1,
  "gnetId": null,
  "links": [],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Datasource",
        "hide": 0,
        "query": "prometheus",
        "refresh": 1,
        "current": null
      },
      {
        "name": "job",
        "type": "query",
        "label": "job",
        "datasource": "$datasource",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "query": "label_values(prometheus_tsdb_head_series, job)",
        "regex": "",
        "current": { "selected": false, "text": "All", "value": "$__all" }
      },
      {
        "name": "namespace",
        "type": "query",
        "label": "namespace",
        "datasource": "$datasource",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "query": "label_values(prometheus_tsdb_head_series{job=~\"$job\"}, namespace)",
        "regex": "",
        "current": { "selected": false, "text": "All", "value": "$__all" }
      },
      {
        "name": "pod",
        "type": "query",
        "label": "pod",
        "datasource": "$datasource",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "query": "label_values(prometheus_tsdb_head_series{job=~\"$job\", namespace=~\"$namespace\"}, pod)",
        "regex": "",
        "current": { "selected": false, "text": "All", "value": "$__all" }
      },
      {
        "name": "instance",
        "type": "query",
        "label": "instance",
        "datasource": "$datasource",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "query": "label_values(prometheus_tsdb_head_series{job=~\"$job\"}, instance)",
        "regex": "",
        "current": { "selected": false, "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Обзор TSDB",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Объем блоков TSDB на диске",
      "description": "prometheus_tsdb_storage_blocks_bytes — текущий суммарный объем на диске для всех блоков TSDB.",
      "datasource": "$datasource",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_storage_blocks_bytes{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "{{job}} {{namespace}} {{pod}} {{instance}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 2,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.7e12 },
              { "color": "red", "value": 0.9e12 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Активные серии (head)",
      "description": "prometheus_tsdb_head_series — число активных временных рядов в head.",
      "datasource": "$datasource",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_head_series{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "{{job}} {{namespace}} {{pod}} {{instance}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5e6 },
              { "color": "red", "value": 1e7 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Чанки в head",
      "description": "prometheus_tsdb_head_chunks — число чанков в head.",
      "datasource": "$datasource",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_head_chunks{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "{{job}} {{namespace}} {{pod}} {{instance}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Загруженные блоки",
      "description": "prometheus_tsdb_blocks_loaded — текущее число загруженных блоков.",
      "datasource": "$datasource",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_blocks_loaded{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "{{job}} {{namespace}} {{pod}} {{instance}}",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "unit": "short", "decimals": 0 },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Динамика и производительность",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Скорость инжеста (samples/sec)",
      "description": "rate(prometheus_tsdb_head_samples_appended_total[5m]) — скорость добавления сэмплов.",
      "datasource": "$datasource",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (rate(prometheus_tsdb_head_samples_appended_total{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "{{job}} {{namespace}} {{pod}} {{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Активные серии и чанки в head",
      "description": "Нагрузка head: серии и чанки.",
      "datasource": "$datasource",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_head_series{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "series | {{job}} {{pod}}",
          "refId": "A"
        },
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_head_chunks{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "chunks | {{job}} {{pod}}",
          "refId": "B"
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "list" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Объем блоков TSDB (GiB)",
      "description": "prometheus_tsdb_storage_blocks_bytes, приведенный к GiB.",
      "datasource": "$datasource",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (prometheus_tsdb_storage_blocks_bytes{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"})",
          "legendFormat": "{{job}} {{namespace}} {{pod}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 2 } },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Head GC: средняя длительность (сек)",
      "description": "Средняя длительность GC head: rate(sum)/rate(count) за 5m.",
      "datasource": "$datasource",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "sum by (job, namespace, pod, instance) (rate(prometheus_tsdb_head_gc_duration_seconds_sum{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"}[5m])) / sum by (job, namespace, pod, instance) (rate(prometheus_tsdb_head_gc_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", pod=~\"$pod\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "{{job}} {{pod}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 } },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "row",
      "title": "Кардинальность и источники",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 9,
      "type": "table",
      "title": "Top jobs по объему инжеста (сэмплы/с)",
      "description": "Суммарная скорость инжеста по job: rate(prometheus_tsdb_head_samples_appended_total[5m]).",
      "datasource": "$datasource",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 25 },
      "targets": [
        {
          "expr": "topk(20, sum by (job) (rate(prometheus_tsdb_head_samples_appended_total{job=~\"$job\"}[5m])))",
          "legendFormat": "{{job}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops", "decimals": 0 },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "footer": { "enablePagination": true, "fields": "", "reducer": ["sum"] }
      },
      "transformations": [
        { "id": "sortBy", "options": { "fields": {}, "sort": [ { "field": "Value", "desc": true } ] } }
      ]
    }
  ]
}
