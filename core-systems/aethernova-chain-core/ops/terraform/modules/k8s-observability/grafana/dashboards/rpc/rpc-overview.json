{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "refresh": "30s",
  "id": null,
  "iteration": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "KPI",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 1,
      "type": "stat",
      "title": "RPS (запросов/с)",
      "description": "Суммарная скорость запросов RPC/HTTP/gRPC",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "decimals": 2 },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rpc_server_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        },
        {
          "refId": "B",
          "expr": "sum(rate(http_server_request_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        },
        {
          "refId": "C",
          "expr": "sum(rate(grpc_server_handled_total{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "reduce", "options": { "reducers": ["lastNotNull"] } }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Успешность, %",
      "description": "Доля успешных ответов",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "percent", "decimals": 2, "min": 0, "max": 100 },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum(rate(rpc_server_duration_seconds_count{status_code=~\"OK|UNSET|2..|3..\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) / sum(rate(rpc_server_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        },
        {
          "refId": "B",
          "expr": "100 * sum(rate(http_server_request_duration_seconds_count{status_code=~\"2..|3..\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) / sum(rate(http_server_request_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        },
        {
          "refId": "C",
          "expr": "100 * sum(rate(grpc_server_handled_total{grpc_code=\"OK\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) / sum(rate(grpc_server_handled_total{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "reduce", "options": { "reducers": ["lastNotNull"] } }
      ]
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Ошибки, %",
      "description": "Доля ошибочных ответов",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "fieldConfig": {
        "defaults": { "unit": "percent", "decimals": 2, "min": 0, "max": 100 },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum(rate(rpc_server_duration_seconds_count{status_code!~\"OK|UNSET|2..|3..\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) / sum(rate(rpc_server_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        },
        {
          "refId": "B",
          "expr": "100 * sum(rate(http_server_request_duration_seconds_count{status_code!~\"2..|3..\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) / sum(rate(http_server_request_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        },
        {
          "refId": "C",
          "expr": "100 * sum(rate(grpc_server_handled_total{grpc_code!=\"OK\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) / sum(rate(grpc_server_handled_total{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval]))"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "reduce", "options": { "reducers": ["lastNotNull"] } }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "P95 латентность, s",
      "description": "Текущая P95 по длительности обработки",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } },
      "fieldConfig": {
        "defaults": { "unit": "s", "decimals": 3 },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(rpc_server_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])))"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])))"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(grpc_server_handling_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])))"
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "reduce", "options": { "reducers": ["lastNotNull"] } }
      ]
    },

    {
      "type": "row",
      "title": "Латентность",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Латентность P50/P95/P99 (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 8 },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "legendFormat": "P50",
          "expr": "histogram_quantile(0.50, sum by (le) (rate(rpc_server_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) + sum by (le) (rate(http_server_request_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) + sum by (le) (rate(grpc_server_handling_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])))"
        },
        {
          "refId": "B",
          "legendFormat": "P95",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(rpc_server_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) + sum by (le) (rate(http_server_request_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) + sum by (le) (rate(grpc_server_handling_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])))"
        },
        {
          "refId": "C",
          "legendFormat": "P99",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(rpc_server_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) + sum by (le) (rate(http_server_request_duration_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])) + sum by (le) (rate(grpc_server_handling_seconds_bucket{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\", instance=~\"$instance\"}[$__rate_interval])))"
        }
      ]
    },

    {
      "type": "row",
      "title": "Ошибки",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Ошибка, % (во времени)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "fieldConfig": { "defaults": { "unit": "percent", "decimals": 2 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "legendFormat": "RPC",
          "expr": "100 * sum(rate(rpc_server_duration_seconds_count{status_code!~\"OK|UNSET|2..|3..\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[$__rate_interval])) / sum(rate(rpc_server_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[$__rate_interval]))"
        },
        {
          "refId": "B",
          "legendFormat": "HTTP",
          "expr": "100 * sum(rate(http_server_request_duration_seconds_count{status_code!~\"2..|3..\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[$__rate_interval])) / sum(rate(http_server_request_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[$__rate_interval]))"
        },
        {
          "refId": "C",
          "legendFormat": "gRPC",
          "expr": "100 * sum(rate(grpc_server_handled_total{grpc_code!=\"OK\", job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[$__rate_interval])) / sum(rate(grpc_server_handled_total{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[$__rate_interval]))"
        }
      ]
    },
    {
      "id": 7,
      "type": "bargauge",
      "title": "Коды ответов (последние 5 минут)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "fieldConfig": { "defaults": { "unit": "reqps", "decimals": 2 }, "overrides": [] },
      "options": { "displayMode": "lcd", "orientation": "horizontal" },
      "targets": [
        {
          "refId": "A",
          "legendFormat": "{{status_code}}",
          "expr": "sum by (status_code) (rate(rpc_server_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[5m]))"
        },
        {
          "refId": "B",
          "legendFormat": "{{status_code}}",
          "expr": "sum by (status_code) (rate(http_server_request_duration_seconds_count{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[5m]))"
        },
        {
          "refId": "C",
          "legendFormat": "{{grpc_code}}",
          "expr": "sum by (grpc_code) (rate(grpc_server_handled_total{job=~\"$job\", namespace=~\"$namespace\", service=~\"$service\"}[5m]))"
        }
      ],
      "transformations": [{ "id": "merge", "options": {} }]
    }
  ],
