  "panels": [
    {
      "type": "timeseries",
      "title": "API latency P99 (s) by verb",
      "description": "histogram_quantile(0.99) по apiserver_request_duration_seconds_bucket",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (verb, le) (rate(apiserver_request_duration_seconds_bucket{job=~\"$job\", verb!~\"WATCH|WATCHLIST\"}[$__rate_interval])))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["last","max","mean"] } }
    },
    {
      "type": "timeseries",
      "title": "API latency P90 (s) by verb",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.90, sum by (verb, le) (rate(apiserver_request_duration_seconds_bucket{job=~\"$job\", verb!~\"WATCH|WATCHLIST\"}[$__rate_interval])))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
