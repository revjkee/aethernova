      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum by (verb, le) (rate(apiserver_request_duration_seconds_bucket{job=~\"$job\", verb!~\"WATCH|WATCHLIST\"}[$__rate_interval])))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "type": "timeseries",
      "title": "QPS by verb",
      "description": "Скорость запросов по apiserver_request_total",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (verb) (rate(apiserver_request_total{job=~\"$job\"}[$__rate_interval]))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "HTTP 5xx (rps)",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (verb) (rate(apiserver_request_total{job=~\"$job\", code=~\"5..\"}[$__rate_interval]))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "HTTP 4xx (rps)",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (verb) (rate(apiserver_request_total{job=~\"$job\", code=~\"4..\"}[$__rate_interval]))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Timeouts/Throttling (504 + 429) (rps)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (verb) (rate(apiserver_request_total{job=~\"$job\", code=~\"429|504\"}[$__rate_interval]))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Inflight requests by kind",
      "description": "apiserver_current_inflight_requests с request_kind",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (request_kind) (apiserver_current_inflight_requests{job=~\"$job\"})",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Queued requests by kind",
      "description": "apiserver_current_inqueue_requests (API Priority and Fairness)",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (request_kind) (apiserver_current_inqueue_requests{job=~\"$job\"})",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    },
    {
      "type": "table",
      "title": "Top P99 latency by resource",
      "description": "Top-10 ресурсов по P99 (subresource=\"\")",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 24 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, histogram_quantile(0.99, sum by (resource, le) (rate(apiserver_request_duration_seconds_bucket{job=~\"$job\", subresource=\"\", verb!~\"WATCH|WATCHLIST\"}[$__rate_interval]))))",
          "datasource": { "type": "prometheus", "uid": "${ds}" },
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "showHeader": true }
    },
    {
      "type": "timeseries",
      "title": "etcd request latency P99 (s)",
      "description": "Через метрики apiserver: etcd_request_duration_seconds_bucket",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (operation, le) (rate(etcd_request_duration_seconds_bucket{job=~\"$job\"}[$__rate_interval])))",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "type": "timeseries",
      "title": "API error rate (%)",
      "description": "((4xx+5xx)/все)×100",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (\n  sum by (verb) (rate(apiserver_request_total{job=~\"$job\", code=~\"4..|5..\"}[$__rate_interval]))\n) / (\n  sum by (verb) (rate(apiserver_request_total{job=~\"$job\"}[$__rate_interval]))\n)",
          "datasource": { "type": "prometheus", "uid": "${ds}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } }
    }
  ],
