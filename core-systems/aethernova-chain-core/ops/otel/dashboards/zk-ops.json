{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "panels": [],
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 50 },
              { "color": "red", "value": 200 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "id": 101,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${ds}" },
          "expr": "sum(zk_prover_queue_depth{cluster=~\"${cluster}\", namespace=~\"${namespace}\", pod=~\"${pod}\", job=~\"${job}\"})",
          "legendFormat": "queue_depth",
          "refId": "A"
        }
      ],
      "title": "Prover Queue Depth",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "green", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "id": 102,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(zk_proofs_total{cluster=~\"${cluster}\", namespace=~\"${namespace}\", pod=~\"${pod}\", job=~\"${job}\"}[5m]))",
          "legendFormat": "proofs/s",
          "refId": "A"
        }
      ],
      "title": "Throughput (proofs/sec)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.02 },
              { "color": "red", "value": 0.05 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "id": 103,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(zk_proof_failures_total{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}[5m]))\n/ clamp_min(sum(rate(zk_proofs_total{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}[5m])), 1)",
          "legendFormat": "failure_rate",
          "refId": "A"
        }
      ],
      "title": "Failure Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth" }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 1 },
      "id": 104,
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(zk_proof_latency_seconds_bucket{cluster=~\"${cluster}\", namespace=~\"${namespace}\", job=~\"${job}\"}[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le) (rate(zk_proof_latency_seconds_bucket{cluster=~\"${cluster}\", namespace=~\"${namespace}\", job=~\"${job}\"}[5m])))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(zk_proof_latency_seconds_bucket{cluster=~\"${cluster}\", namespace=~\"${namespace}\", job=~\"${job}\"}[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "Proof Latency (p50/p90/p99)",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "id": 200,
      "panels": [],
      "title": "ZK Batch & Node State",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 8 },
              { "color": "green", "value": 16 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 8 },
      "id": 201,
      "options": {
        "displayMode": "gradient",
        "min": 0,
        "orientation": "horizontal",
        "showUnfilled": true
      },
      "targets": [
        {
          "expr": "avg(zk_batch_size{cluster=~\"${cluster}\", namespace=~\"${namespace}\", job=~\"${job}\"})",
          "legendFormat": "batch_size",
          "refId": "A"
        }
      ],
      "title": "Average Batch Size",
      "type": "bargauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 8 },
      "id": 202,
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "targets": [
        {
          "expr": "sum by (reason) (rate(zk_proof_failures_total{cluster=~\"${cluster}\", namespace=~\"${namespace}\", job=~\"${job}\"}[5m]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "title": "Failures by Reason (5m rate)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 8 },
      "id": 203,
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "single" } },
      "targets": [
        {
          "expr": "max(zk_node_sync_height{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}) by (instance)",
          "legendFormat": "node: {{instance}}",
          "refId": "A"
        },
        {
          "expr": "max(zk_network_height{cluster=~\"${cluster}\"})",
          "legendFormat": "network",
          "refId": "B"
        }
      ],
      "title": "Node Sync Height vs Network",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "id": 300,
      "panels": [],
      "title": "System Resources (Pods/Nodes)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "id": 301,
      "targets": [
        {
          "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{namespace=~\"${namespace}\", pod=~\"${pod}\", image!=\"\", container!=\"POD\"}[5m]))",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
      "title": "Pod CPU (cores)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "cores" }, "overrides": [] }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "id": 302,
      "targets": [
        {
          "expr": "sum by (pod) (container_memory_working_set_bytes{namespace=~\"${namespace}\", pod=~\"${pod}\", image!=\"\", container!=\"POD\"})",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
      "title": "Pod Memory (working set)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 24 },
      "id": 303,
      "targets": [
        {
          "expr": "sum by (pod) (rate(container_network_receive_bytes_total{namespace=~\"${namespace}\", pod=~\"${pod}\"}[5m]))",
          "legendFormat": "{{pod}} RX",
          "refId": "A"
        },
        {
          "expr": "sum by (pod) (rate(container_network_transmit_bytes_total{namespace=~\"${namespace}\", pod=~\"${pod}\"}[5m]))",
          "legendFormat": "{{pod}} TX",
          "refId": "B"
        }
      ],
      "title": "Pod Network (RX/TX)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 24 },
      "id": 304,
      "targets": [
        {
          "expr": "sum by (pod) (rate(container_fs_reads_bytes_total{namespace=~\"${namespace}\", pod=~\"${pod}\"}[5m]))",
          "legendFormat": "{{pod}} reads",
          "refId": "A"
        },
        {
          "expr": "sum by (pod) (rate(container_fs_writes_bytes_total{namespace=~\"${namespace}\", pod=~\"${pod}\"}[5m]))",
          "legendFormat": "{{pod}} writes",
          "refId": "B"
        }
      ],
      "title": "Pod Filesystem IO",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] }
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "id": 400,
      "panels": [],
      "title": "GPU Utilization (if present)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "id": 401,
      "targets": [
        {
          "expr": "(avg(DCGM_FI_DEV_GPU_UTIL{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}) by (gpu)) or (avg(nvidia_gpu_duty_cycle{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}) by (gpu))",
          "legendFormat": "gpu{{gpu}}",
          "refId": "A"
        }
      ],
      "title": "GPU Utilization",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "id": 402,
      "targets": [
        {
          "expr": "(avg(DCGM_FI_DEV_MEM_COPY_UTIL{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}) by (gpu)) or (avg(nvidia_gpu_memory_used_bytes{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}) by (gpu))",
          "legendFormat": "gpu{{gpu}}",
          "refId": "A"
        }
      ],
      "title": "GPU Memory / Copy Util",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] }
    },

    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "id": 500,
      "panels": [],
      "title": "Service SLOs",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 41 },
      "id": 501,
      "targets": [
        {
          "expr": "1 - (sum(rate(zk_proof_failures_total{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}[30d]))\n/ clamp_min(sum(rate(zk_proofs_total{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}[30d])), 1))",
          "legendFormat": "30d success ratio",
          "refId": "A"
        }
      ],
      "title": "Success Ratio (30d)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "percentunit" }, "overrides": [] }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${ds}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 41 },
      "id": 502,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(zk_proof_latency_seconds_bucket{cluster=~\"${cluster}\", namespace=~\"${namespace}\"}[30d])))",
          "legendFormat": "p95 (30d)",
          "refId": "A"
        }
      ],
      "title": "Latency p95 (30d)",
      "type": "timeseries",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["aethernova", "otel", "zk", "ops", "slo"],
  "templating": {
    "list": [
      {
        "name": "ds",
        "type": "datasource",
        "query": "prometheus",
        "label": "Datasource",
        "current": { "selected": true, "text": "Prometheus", "value": "Prometheus" }
      },
      {
        "name": "cluster",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds}" },
        "query": "label_values(up, cluster)",
        "label": "Cluster",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": ".*" },
        "regex": ""
      },
      {
        "name": "namespace",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds}" },
        "query": "label_values(zk_proofs_total, namespace)",
        "label": "Namespace",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": ".*" }
      },
      {
        "name": "job",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds}" },
        "query": "label_values(zk_proofs_total, job)",
        "label": "Job",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": ".*" }
      },
      {
        "name": "pod",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${ds}" },
        "query": "label_values(zk_proofs_total{namespace=~\"${namespace}\"}, pod)",
        "label": "Pod",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": ".*" }
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {
    "hidden": false,
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "timezone": "",
  "title": "ZK Ops (Aethernova / OpenTelemetry)",
  "uid": "zk-ops-aethernova",
  "version": 1,
  "weekStart": ""
}
