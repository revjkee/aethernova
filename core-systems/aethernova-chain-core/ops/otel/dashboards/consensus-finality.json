      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "type": "stat",
      "title": "Finalized block height",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by () (aethernova_consensus_finalized_block_number{job=~\"$job\", instance=~\"$instance\", network=~\"$network\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 0 },
      "type": "stat",
      "title": "Head block height",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by () (aethernova_consensus_head_block_number{job=~\"$job\", instance=~\"$instance\", network=~\"$network\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "type": "timeseries",
      "title": "Finality lag (blocks): head âˆ’ finalized",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (network) (aethernova_consensus_head_block_number{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}) - max by (network) (aethernova_consensus_finalized_block_number{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"})"
        }
      ],
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" },
        "tooltip": { "mode": "multi" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "type": "timeseries",
      "title": "Time to finality (seconds)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (network) (aethernova_consensus_time_to_finality_seconds{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 10 },
              { "color": "red", "value": 30 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "table" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 },
      "type": "timeseries",
      "title": "Finalized blocks per minute",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (network) (rate(aethernova_consensus_finalized_blocks_total{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}[$__rate_interval])) * 60"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "blocks/min"
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "type": "timeseries",
      "title": "Epoch finalization rate (epochs/min)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (network) (rate(aethernova_consensus_finalized_epochs_total{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}[$__rate_interval])) * 60"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "opm" },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true }
      }
    },
    {
      "id": 7,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "type": "timeseries",
      "title": "Validator participation ratio (%)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (network) (aethernova_validator_participation_ratio{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 66 },
              { "color": "green", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true }
      }
    },
    {
      "id": 8,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 21 },
      "type": "timeseries",
      "title": "Fork rate (per minute)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (network) (rate(aethernova_consensus_forks_total{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}[$__rate_interval])) * 60"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "opm" },
        "overrides": []
      },
      "options": { "legend": { "showLegend": true } }
    },
    {
      "id": 9,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 25 },
      "type": "timeseries",
      "title": "Finality distance (blocks) per validator set",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (validator_set, network) (aethernova_consensus_head_block_number{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}) - max by (validator_set, network) (aethernova_consensus_finalized_block_number{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"})"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      },
      "options": { "legend": { "showLegend": true, "displayMode": "table" } }
    },
    {
      "id": 10,
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 29 },
      "type": "stat",
      "title": "p95 time to finality (last 1h)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.95, aethernova_consensus_time_to_finality_seconds{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}[1h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 15 },
              { "color": "red", "value": 45 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      }
    },
    {
      "id": 11,
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 29 },
      "type": "stat",
      "title": "Reorgs (last 1h)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(aethernova_consensus_reorg_events_total{job=~\"$job\",instance=~\"$instance\",network=~\"$network\"}[1h])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      }
    },
    {
      "id": 12,
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 34 },
      "type": "table",
      "title": "Finality by client (last sample)",
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (client, network) (aethernova_consensus_finalized_block_number{job=~\"$job\",instance=~\"$instance\",network=~\"$network\",client=~\"$client\"})"
        },
        {
          "refId": "B",
          "expr": "max by (client, network) (aethernova_consensus_head_block_number{job=~\"$job\",instance=~\"$instance\",network=~\"$network\",client=~\"$client\"})"
        }
      ],
      "transformations": [
        { "id": "merge", "options": { "reducers": ["lastNotNull"] } },
        { "id": "organize", "options": { "excludeByName": {}, "indexByName": {} } }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      },
      "options": {
        "showHeader": true
      }
    }
  ],
