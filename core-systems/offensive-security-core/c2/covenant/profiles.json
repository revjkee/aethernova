{
  "profiles": [
    {
      "name": "default",
      "description": "Оптимальный профиль для скрытного и устойчивого соединения",
      "httpConfig": {
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/117.0.0.0 Safari/537.36",
        "getPath": "/api/checkin",
        "postPath": "/api/update",
        "headers": {
          "Accept": "application/json, text/plain, */*",
          "Content-Type": "application/json"
        },
        "useSSL": true,
        "jitter": 15,
        "sleepMin": 10,
        "sleepMax": 30
      },
      "dnsConfig": {
        "enabled": true,
        "sleepMin": 10,
        "sleepMax": 30,
        "maxRetries": 3
      },
      "proxyConfig": {
        "enabled": false,
        "proxyList": []
      },
      "encryption": {
        "algorithm": "aes256",
        "keyRotation": true
      },
      "logging": {
        "enabled": false,
        "level": "error"
      },
      "retryPolicy": {
        "maxRetries": 5,
        "retryInterval": 60
      }
    }
  ]
}
