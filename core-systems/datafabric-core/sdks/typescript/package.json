{
  "name": "@datafabric/core-sdk",
  "version": "0.1.0-rc.0",
  "description": "Industrial-grade TypeScript SDK for DataFabric Core APIs and schema bindings",
  "license": "Apache-2.0",
  "author": {
    "name": "DataFabric Core Engineering",
    "email": "devops@datafabric.example"
  },
  "homepage": "https://datafabric.example/sdk/typescript",
  "repository": {
    "type": "git",
    "url": "ssh://git@code.datafabric.example/datafabric-core.git",
    "directory": "sdks/typescript"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.datafabric.example"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "clean": "rimraf dist .turbo node_modules/.cache",
    "build": "npm run clean && tsc -p tsconfig.build.json && tsc -p tsconfig.build.cjs.json",
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:proto": "buf generate",
    "generate:jsonschema": "node scripts/gen-jsonschema-types.mjs",
    "release": "npm publish --provenance",
    "security:audit": "npm audit --audit-level=high",
    "ci:verify": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "dependencies": {
    "@apollo/client": "^3.10.0",
    "@bufbuild/protobuf": "^1.7.2",
    "graphql": "^16.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^7.12.1",
    "@typescript-eslint/parser": "^7.12.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3",
    "vitest": "^1.6.0",
    "buf": "^1.30.0"
  },
  "overrides": {
    "minimatch": "^9.0.4",
    "lodash": "^4.17.21"
  },
  "packageManager": "npm@9.9.2",
  "sideEffects": false
}
