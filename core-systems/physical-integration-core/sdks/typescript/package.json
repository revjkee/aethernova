{
  "name": "@aethernova/physical-integration-core-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Physical Integration Core: safety, telemetry, OTA and policy evaluation clients.",
  "license": "Apache-2.0",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "engines": {
    "node": ">=18.17 <23",
    "npm": ">=9"
  },
  "packageManager": "pnpm@9.7.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --minify --treeshake --clean",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier -c .",
    "format:write": "prettier -w .",
    "test": "vitest run --reporter=default --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm typecheck && pnpm test",
    "prepare": "husky install",
    "release": "pnpm build && changeset publish"
  },
  "peerDependencies": {
    "protobufjs": "^7.2.6"
  },
  "peerDependenciesMeta": {
    "protobufjs": {
      "optional": true
    }
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/node": "^20.16.2",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "c8": "^10.1.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "sdk",
    "typescript",
    "iot",
    "ota",
    "safety",
    "protobuf",
    "neurocity",
    "physical-integration"
  ]
}
