{
  "nginx.conf": {
    "server": {
      "listen": 80,
      "server_name": "internal.api.corp.local",
      "location": "/admin": {
        "proxy_pass": "http://127.0.0.1:9999",
        "auth_basic": "Restricted Area",
        "auth_basic_user_file": "/etc/nginx/.htpasswd"
      }
    },
    "metadata": {
      "type": "webserver",
      "version": "1.18.0",
      "expose_fake_panel": true,
      "honeypot": true
    }
  },

  "postgresql.conf": {
    "listen_addresses": "127.0.0.1",
    "port": 5433,
    "max_connections": 100,
    "password_encryption": "md5",
    "logging_collector": true,
    "metadata": {
      "type": "database",
      "version": "12.5",
      "has_fake_admin": true,
      "honeypot": true
    }
  },

  "docker-compose.yml": {
    "version": "3.8",
    "services": {
      "app": {
        "image": "internal/service:prod",
        "ports": ["8080:80"],
        "volumes": ["./app:/var/www/html"]
      },
      "db": {
        "image": "postgres:12.5",
        "environment": {
          "POSTGRES_PASSWORD": "Admin1234"
        }
      }
    },
    "metadata": {
      "type": "container",
      "includes_backdoor": true,
      "honeypot": true
    }
  },

  "ssh_config": {
    "Port": 2222,
    "PermitRootLogin": "yes",
    "PasswordAuthentication": "yes",
    "ChallengeResponseAuthentication": "no",
    "UsePAM": "yes",
    "Banner": "/etc/issue",
    "metadata": {
      "type": "ssh",
      "vulnerable_port": true,
      "backdoor_user": "honeypot",
      "honeypot": true
    }
  },

  "kubeconfig": {
    "clusters": [
      {
        "name": "staging-cluster",
        "cluster": {
          "server": "https://10.0.0.1:6443",
          "insecure-skip-tls-verify": true
        }
      }
    ],
    "users": [
      {
        "name": "admin",
        "user": {
          "token": "fake-super-long-token-for-trap"
        }
      }
    ],
    "metadata": {
      "type": "k8s",
      "fake_cluster": true,
      "honeypot": true
    }
  }
}
