{
  "id": null,
  "uid": "llmops-overview",
  "title": "LLMOps System Overview",
  "tags": ["llmops", "overview", "ai", "performance", "quality"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Total Requests (5m)",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(llm_total_requests[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Token Usage (avg)",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg_over_time(llm_token_usage_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Hallucination Rate",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg_over_time(llm_hallucination_score[5m])",
          "refId": "A"
        }
      ],
      "thresholds": "0.3,0.6"
    },
    {
      "type": "stat",
      "title": "Prompt Injection Detected",
      "id": 4,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(llm_prompt_injection_detected_total[10m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency (P95)",
      "id": 5,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(llm_response_latency_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Retry Rate",
      "id": 6,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(llm_inference_retries_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Filtered Outputs",
      "id": 7,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(llm_output_filter_rejections_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Model Load Latency",
      "id": 8,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "avg_over_time(llm_model_load_latency_seconds[10m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top Error Types",
      "id": 9,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(5, rate(llm_internal_error_total[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Toxicity Events",
      "id": 10,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(llm_toxicity_score_above_threshold_total[10m])",
          "refId": "A"
        }
      ]
    }
  ]
}
