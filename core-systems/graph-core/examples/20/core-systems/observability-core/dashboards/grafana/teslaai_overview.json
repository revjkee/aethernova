{
  "id": null,
  "uid": "teslaai-overview",
  "title": "TeslaAI System Overview",
  "tags": ["overview", "platform", "ai", "security", "tokenomics"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Total Active Agents",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(ai_agents_active_total)",
          "refId": "A"
        }
      ],
      "colors": ["green", "orange", "red"],
      "thresholds": "10,50"
    },
    {
      "type": "stat",
      "title": "Zero Trust Violations",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(zero_trust_policy_violation_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "RL Training Jobs (Active)",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rl_training_active_jobs)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Token Circulation Rate",
      "id": 4,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(tokenomics_circulation_total[5m])",
          "refId": "A"
        }
      ],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          { "color": "green", "value": null },
          { "color": "orange", "value": 500 },
          { "color": "red", "value": 1000 }
        ]
      }
    },
    {
      "type": "stat",
      "title": "Anomaly Events (AI)",
      "id": 5,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(ai_anomaly_event_total[10m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top 5 Threat Sources",
      "id": 6,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(5, threat_score_by_source)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency (API Gateway)",
      "id": 7,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_gateway_request_duration_bucket[5m])) by (le))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Staking Activity (DAO)",
      "id": 8,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(dao_staking_events_total)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Vault Access Events",
      "id": 9,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(secrets_vault_access_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "heatmap",
      "title": "Training Load (Agents)",
      "id": 10,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rl_agent_training_cpu_usage",
          "refId": "A"
        }
      ]
    }
  ]
}
