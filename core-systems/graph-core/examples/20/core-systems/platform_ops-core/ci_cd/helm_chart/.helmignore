# Patterns to ignore when packaging Helm charts

# Ignore local Helm configuration files
.helm/

# Ignore version control system directories
.git/
.gitignore

# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore Python artifacts
__pycache__/
*.pyc
*.pyo

# Ignore test files
tests/
*.md
README.md

# Ignore Docker files
Dockerfile
docker-compose.yml

# Ignore logs and temp files
*.log
tmp/
temp/

# Ignore IDE/editor files
.vscode/
.idea/
*.swp
*.swo
